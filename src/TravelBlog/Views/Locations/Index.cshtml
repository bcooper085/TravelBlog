@model IEnumerable<TravelBlog.Models.Location>

<html>
<head>
    <title>Travel Blog</title>
</head>
<body>
    <h2>Locations</h2>
    <h4>@Html.DisplayNameFor(model => model.Name)</h4>
    <ul>
        @foreach (var listLocation in Model)
        {
            <li>@Html.DisplayFor(modelItem => listLocation.Name) | @Html.ActionLink("Details", "Details", new { id = listLocation.LocationId })</li>
        }
    </ul>
</body>
</html>